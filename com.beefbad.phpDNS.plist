<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.beefbad.phpDNS</string>
	<key>KeepAlive</key>
	<true/>
	<key>RunAtLoad</key>
	<true/>
	<key>WorkingDirectory</key>
	<string>/Users/apple/.phpDNS</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Users/apple/.phpDNS/dnsfix.sh</string>
	</array>
	<key>ThrottleInterval</key>
	<integer>1</integer>
	<key>Nice</key>
	<integer>1</integer>
	<key>UserName</key>
	<string>root</string>
	<key>StandardErrorPath</key>
	<string>/Users/apple/Library/Logs/phpDNS.log</string>
	<key>StandardOutPath</key>
	<string>/Users/apple/Library/Logs/phpDNS.log</string>
</dict>
</plist>